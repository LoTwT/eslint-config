{
  "name": "@ayingott/eslint-config",
  "type": "module",
  "version": "1.0.3",
  "packageManager": "pnpm@9.1.3",
  "description": "@ayingott/eslint-config",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.0.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.19.0",
    "@ayingott/eslint-config-prettier": "workspace:*",
    "@ayingott/prettier-config": "^0.3.3"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
