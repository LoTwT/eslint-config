{
  "name": "@ayingott/eslint-config",
  "version": "0.7.7",
  "description": "@ayingott/eslint-config",
  "type": "module",
  "packageManager": "pnpm@8.14.2",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@ayingott/eslint-config-basic": "workspace:*",
    "@ayingott/eslint-config-markdown": "workspace:*",
    "@ayingott/eslint-config-prettier": "workspace:*",
    "@ayingott/eslint-config-react": "workspace:*",
    "@ayingott/eslint-config-unocss": "workspace:*",
    "@ayingott/eslint-config-vue": "workspace:*",
    "@ayingott/sucrose": "^0.0.34"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "pnpm": "^8.14.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
