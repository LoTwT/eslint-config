  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@ayingott/sucrose": "^0.0.34",
    "@sxzz/eslint-config": "^3.7.4",
    "eslint-plugin-n": "^16.3.1"
  },
  "devDependencies": {
    "eslint-define-config": "^2.0.0",
    "pnpm": "^8.10.5",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2"
  }
