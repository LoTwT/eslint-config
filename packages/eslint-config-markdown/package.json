{
  "name": "@ayingott/eslint-config-markdown",
  "version": "0.7.4",
  "description": "@ayingott/eslint-config-markdown",
  "type": "module",
  "packageManager": "pnpm@8.10.5",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LoTwT/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/LoTwT/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
