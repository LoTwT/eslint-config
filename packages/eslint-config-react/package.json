  "description": "@ayingott/eslint-config-react",
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "author": "LoTwT <l709937065@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/LoTwT/eslint-config#readme",
